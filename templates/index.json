      "name": "t:sections.custom-liquid.presets.name",
      "settings": {
        "custom_liquid": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n  <meta charset=\"UTF-8\" />\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\" />\n  <title>Weighted Spin Wheel Popup</title>\n  <style>\n    /* Overlay */\n    #spin-popup-modal {\n      display: none;\n      position: fixed;\n      z-index: 9999;\n      left: 0;\n      top: 0;\n      width: 100vw;\n      height: 100vh;\n      background: rgba(0, 0, 0, 0.65);\n      justify-content: center;\n      align-items: center;\n    }\n\n    /* Popup box */\n    .spin-popup-box {\n      background: #fff;\n      padding: 32px 24px 28px 24px;\n      border-radius: 18px;\n      max-width: 390px;\n      width: 94vw;\n      box-shadow: 0 8px 32px rgba(0, 0, 0, 0.18);\n      text-align: center;\n      position: relative;\n    }\n\n    /* Close button */\n    #spin-popup-close {\n      position: absolute;\n      top: 8px;\n      right: 12px;\n      background: none;\n      border: none;\n      font-size: 1.5em;\n      cursor: pointer;\n    }\n\n    .spin-wheel-svg-wrap {\n      margin: 0 auto 16px auto;\n      width: 270px;\n      height: 270px;\n      position: relative;\n    }\n\n    #spin-pointer {\n      position: absolute;\n      left: 50%;\n      top: -22px;\n      transform: translateX(-50%);\n      width: 0;\n      height: 0;\n      border-left: 18px solid transparent;\n      border-right: 18px solid transparent;\n      border-bottom: 28px solid #fff;\n      filter: drop-shadow(0 2px 4px #e74c3c88);\n      z-index: 2;\n    }\n\n    #spin-wheel-svg {\n      width: 100%;\n      height: 100%;\n      border-radius: 50%;\n      background: #fff;\n      box-shadow: 0 2px 12px #f3c62333;\n    }\n\n    .spin-segment-label {\n      font-family: inherit;\n      font-size: 10px;\n      font-weight: bold;\n      letter-spacing: 0.5px;\n      fill: #fff;\n      text-anchor: middle;\n      dominant-baseline: middle;\n      pointer-events: none;\n      text-shadow: 0 1px 4px #0003;\n    }\n\n    .spin-btn-center {\n      position: absolute;\n      left: 50%;\n      top: 50%;\n      transform: translate(-50%, -50%);\n      width: 92px;\n      height: 92px;\n      background: #fff;\n      border-radius: 50%;\n      box-shadow: 0 2px 12px #f3c62355;\n      display: flex;\n      flex-direction: column;\n      align-items: center;\n      justify-content: center;\n      z-index: 2;\n      border: 4px solid #f3c623;\n    }\n\n    #spin-btn {\n      width: 80px;\n      height: 80px;\n      border-radius: 50%;\n      background: #fff;\n      border: 2px solid #f3c623;\n      color: #e74c3c;\n      font-size: 1.35em;\n      font-weight: bold;\n      cursor: pointer;\n      transition: background 0.2s;\n    }\n\n    #spin-btn:active {\n      background: #f3c62322;\n    }\n\n    #spin-popup-form {\n      margin-top: 8px;\n    }\n\n    #spin-popup-form input[type=\"tel\"] {\n      width: 100%;\n      padding: 8px 10px;\n      margin-bottom: 14px;\n      border-radius: 6px;\n      border: 1px solid #ccc;\n      font-size: 1em;\n    }\n\n    #spin-popup-form button {\n      width: 100%;\n      background: #f3c623;\n      color: #222;\n      font-weight: bold;\n      padding: 10px 0;\n      border: none;\n      border-radius: 6px;\n      font-size: 1.1em;\n      cursor: pointer;\n    }\n\n    #spin-result {\n      display: none;\n      margin-top: 14px;\n      font-size: 1.23em;\n      font-weight: bold;\n    }\n\n    @media (max-width: 500px) {\n      .spin-wheel-svg-wrap {\n        width: 98vw;\n        height: 98vw;\n        max-width: 330px;\n        max-height: 330px;\n      }\n    }\n  </style>\n</head>\n<body>\n  <!-- Optional trigger button -->\n  <button onclick=\"openSpinPopup()\">Open Spin Wheel</button>\n\n  <!-- Popup Structure -->\n  <div id=\"spin-popup-modal\">\n    <div class=\"spin-popup-box\">\n      <button id=\"spin-popup-close\">&times;</button>\n      <div\n        style=\"\n          font-size: 1.18em;\n          font-weight: 600;\n          margin-bottom: 8px;\n          color: #fff;\n          background: linear-gradient(90deg, #ff6e7f, #f3c623 80%);\n          border-radius: 8px 8px 0 0;\n          padding: 8px 0 6px 0;\n        \"\n      >\n        Play To Win Amazing Offers<br />On Your Cart\n      </div>\n      <div class=\"spin-wheel-svg-wrap\">\n        <div id=\"spin-pointer\"></div>\n        <svg id=\"spin-wheel-svg\" viewBox=\"0 0 300 300\"></svg>\n        <div class=\"spin-btn-center\">\n          <button id=\"spin-btn\">SPIN</button>\n        </div>\n      </div>\n      <form id=\"spin-popup-form\">\n        <input\n          type=\"tel\"\n          id=\"spin-phone\"\n          placeholder=\"Enter phone number\"\n          maxlength=\"15\"\n          required\n        />\n        <button type=\"submit\" id=\"spin-submit\">Submit &amp; Claim Offer</button>\n      </form>\n      <div id=\"spin-result\"></div>\n    </div>\n  </div>\n\n  <script>\n    // --- Spin Wheel Offers (5 segments) ---\n    const spinOffers = [\n      { label: \"Rs 50 off\", color: \"#ffb36b\" },\n      { label: \"Rs 150 off\", color: \"#ff6e7f\" },\n      { label: \"5% off\", color: \"#e1a2c4\" },\n      { label: \"6% off\", color: \"#ffb36b\" },\n      { label: \"8% on order\\nabove 6999\", color: \"#ff6e7f\" },\n    ];\n\n    // Corresponding probabilities (in percent, summing to 100)\n    const spinWeights = [0, 0, 30, 20, 50];\n\n    // Draw the SVG Wheel\n    function drawSpinWheel() {\n      const svg = document.getElementById(\"spin-wheel-svg\");\n      svg.innerHTML = \"\";\n      const n = spinOffers.length;\n      const cx = 150, cy = 150, r = 140;\n\n      for (let i = 0; i < n; i++) {\n        const startAngle = (i / n) * 2 * Math.PI;\n        const endAngle = ((i + 1) / n) * 2 * Math.PI;\n        const x1 = cx + r * Math.cos(startAngle), y1 = cy + r * Math.sin(startAngle);\n        const x2 = cx + r * Math.cos(endAngle),   y2 = cy + r * Math.sin(endAngle);\n        const largeArc = endAngle - startAngle > Math.PI ? 1 : 0;\n        const pathData = [\n          `M ${cx} ${cy}`,\n          `L ${x1} ${y1}`,\n          `A ${r} ${r} 0 ${largeArc} 1 ${x2} ${y2}`,\n          \"Z\"\n        ].join(\" \");\n\n        // Create slice\n        const path = document.createElementNS(\"http://www.w3.org/2000/svg\", \"path\");\n        path.setAttribute(\"d\", pathData);\n        path.setAttribute(\"fill\", spinOffers[i].color);\n        svg.appendChild(path);\n\n        // Text label\n        const angle = (startAngle + endAngle) / 2;\n        const labelX = +(cx + 90 * Math.cos(angle)).toFixed(1);\n        const labelY = +(cy + 90 * Math.sin(angle)).toFixed(1);\n        const text = document.createElementNS(\"http://www.w3.org/2000/svg\", \"text\");\n        text.setAttribute(\"x\", labelX);\n        text.setAttribute(\"y\", labelY);\n        text.setAttribute(\"class\", \"spin-segment-label\");\n        text.setAttribute(\"transform\", `rotate(${(angle * 180) / Math.PI + 90},${labelX},${labelY})`);\n\n        // Break long labels into two lines if needed\n        let lbl = spinOffers[i].label.trim().replace(/\\s+/g, \" \");\n        if (!lbl.includes(\"\\n\") && lbl.length > 18) {\n          const mid = Math.floor(lbl.length / 2);\n          let spaceIdx = lbl.lastIndexOf(\" \", mid);\n          if (spaceIdx === -1 || spaceIdx < 3) spaceIdx = lbl.indexOf(\" \", mid);\n          if (spaceIdx !== -1) {\n            lbl = lbl.slice(0, spaceIdx) + \"\\n\" + lbl.slice(spaceIdx + 1);\n          }\n        }\n        const lines = lbl.split(\"\\n\");\n        if (lines.length === 2) {\n          text.innerHTML = `<tspan x=\"${labelX}\" dy=\"-0.2em\">${lines[0]}</tspan><tspan x=\"${labelX}\" dy=\"1.1em\">${lines[1]}</tspan>`;\n        } else {\n          text.innerHTML = `<tspan x=\"${labelX}\" dy=\"0.5em\">${lines[0]}</tspan>`;\n        }\n        svg.appendChild(text);\n      }\n\n      // Outer border\n      const circle = document.createElementNS(\"http://www.w3.org/2000/svg\", \"circle\");\n      circle.setAttribute(\"cx\", cx);\n      circle.setAttribute(\"cy\", cy);\n      circle.setAttribute(\"r\", r);\n      circle.setAttribute(\"fill\", \"none\");\n      circle.setAttribute(\"stroke\", \"#fff\");\n      circle.setAttribute(\"stroke-width\", \"10\");\n      svg.appendChild(circle);\n    }\n    drawSpinWheel();\n\n    // Helper: pick random index based on spinWeights\n    function pickWeightedIndex(weights) {\n      const total = weights.reduce((sum, w) => sum + w, 0);\n      let rand = Math.random() * total;\n      for (let i = 0; i < weights.length; i++) {\n        if (rand < weights[i]) return i;\n        rand -= weights[i];\n      }\n      return weights.length - 1; // fallback\n    }\n\n    // === Wheel & Form Logic ===\n    (function () {\n      const modal = document.getElementById(\"spin-popup-modal\");\n      const closeBtn = document.getElementById(\"spin-popup-close\");\n      const form = document.getElementById(\"spin-popup-form\");\n      const phoneInput = document.getElementById(\"spin-phone\");\n      const resultDiv = document.getElementById(\"spin-result\");\n      const spinBtn = document.getElementById(\"spin-btn\");\n      const submitBtn = document.getElementById(\"spin-submit\");\n      const wheelSVG = document.getElementById(\"spin-wheel-svg\");\n      let spinning = false, resultIdx = null;\n\n      // Replace with your backend endpoint (must allow CORS)\n      const API_ENDPOINT =\n        \"https://fc61-2401-4900-8898-782e-f06f-39af-5338-fbcb.ngrok-free.app/api/contact/singlecontact/spinwheelcontact\";\n\n      window.openSpinPopup = function () {\n        modal.style.display = \"flex\";\n      };\n      closeBtn.onclick = () => {\n        modal.style.display = \"none\";\n        resetSpin();\n      };\n      modal.onclick = (e) => {\n        if (e.target === modal) closeBtn.onclick();\n      };\n      function resetSpin() {\n        resultDiv.style.display = \"none\";\n        form.style.display = \"\";\n        phoneInput.value = \"\";\n        spinBtn.disabled = false;\n        submitBtn.disabled = false;\n        wheelSVG.style.transition = \"\";\n        wheelSVG.style.transform = \"\";\n        spinning = false;\n        resultIdx = null;\n      }\n\n      spinBtn.onclick = function () {\n        if (spinning) return;\n        spinning = true;\n        spinBtn.disabled = true;\n        resultDiv.style.display = \"none\";\n\n        // Pick index by weights\n        resultIdx = pickWeightedIndex(spinWeights);\n\n        const segAngle = 360 / spinOffers.length;\n        // 6 full spins + land on chosen slice\n        const stopAngle =\n          360 * 6 + (360 - resultIdx * segAngle - segAngle / 2);\n\n        wheelSVG.style.transition = \"transform 3.4s cubic-bezier(.18,.75,.36,1.08)\";\n        wheelSVG.style.transform = `rotate(${stopAngle}deg)`;\n\n        setTimeout(() => {\n          resultDiv.style.display = \"block\";\n          resultDiv.textContent = spinOffers[resultIdx].label.replace(/\\n/g, \" \");\n          resultDiv.style.color = (spinWeights[resultIdx] === 0) ? \"#e74c3c\" : \"#27ae60\";\n          form.style.display = \"\";\n          spinning = false;\n          spinBtn.disabled = false;\n        }, 3500);\n      };\n\n      form.onsubmit = async function (e) {\n        e.preventDefault();\n        if (resultIdx === null) {\n          spinBtn.focus();\n          return;\n        }\n        const phone = phoneInput.value.trim();\n        if (!phone) {\n          phoneInput.focus();\n          return;\n        }\n\n        submitBtn.disabled = true;\n        resultDiv.style.display = \"block\";\n        resultDiv.textContent = \"Sending...\";\n        resultDiv.style.color = \"#f3c623\";\n\n        // Base payload\n        const payload = {\n          senderId: \"678f94999ae41a0d7d5e773e\",\n          number: phone,\n          name: \"Shopify Spin by Wheel\",\n          tags: \"Spin by Wheel\",\n          source: \"Spin by Wheel\",\n        };\n\n        // Normalize label (remove newline)\n        const rawLabel = spinOffers[resultIdx].label;\n        const label = rawLabel.replace(/\\n/g, \" \").trim();\n\n        // 1) “% on order above X” (only segment 5)\n        if (label.match(/%/) && label.match(/above\\s+\\d+/i)) {\n          const percMatch = label.match(/(\\d+)\\s*%/);\n          const percentage = percMatch ? percMatch[1] : \"\";\n          const minMatch = label.match(/above\\s+(\\d+)/i);\n          let minAmount = minMatch ? minMatch[1] : \"\";\n          // If you want to force a slightly lower minimum:\n          // minAmount = \"6900\";\n          payload.minAmount = minAmount;\n          payload.percentage = percentage;\n        }\n        // 2) “Y% off” (segments 3 and 4)\n        else if (label.match(/^\\d+\\s*%\\s*off$/i)) {\n          const percMatch = label.match(/(\\d+)\\s*%/);\n          const percentage = percMatch ? percMatch[1] : \"\";\n          // Minimum ₹1\n          payload.minAmount = \"1\";\n          payload.percentage = percentage;\n        }\n        // 3) “Rs X off” (segments 1 and 2)\n        else if (label.match(/Rs\\s*\\d+/i)) {\n          const amtMatch = label.match(/Rs\\s*(\\d+)/i);\n          const amount = amtMatch ? amtMatch[1] : \"\";\n          payload.amount = amount;\n        }\n        // 4) Zero‐weight segments never actually get chosen.\n        //    But if someone tries to submit anyway, flag noDiscount.\n        else {\n          payload.noDiscount = true;\n        }\n\n        try {\n          const res = await fetch(API_ENDPOINT, {\n            method: \"POST\",\n            headers: { \"Content-Type\": \"application/json\" },\n            body: JSON.stringify(payload),\n          });\n          const text = await res.text();\n          if (!res.ok) throw new Error(text || \"HTTP \" + res.status);\n\n          resultDiv.textContent = \"Offer claimed! Our team will contact you.\";\n          resultDiv.style.color = \"#27ae60\";\n          form.style.display = \"none\";\n        } catch (err) {\n          console.error(\"[Spin Popup] error:\", err);\n          resultDiv.textContent = \"Something went wrong. Please try again!\";\n          resultDiv.style.color = \"#e74c3c\";\n          submitBtn.disabled = false;\n        }\n      };\n\n      // Auto-open after 5s (remove if you prefer manual trigger)\n      setTimeout(() => (modal.style.display = \"flex\"), 5000);\n    })();\n  </script>\n</body>\n</html>",
